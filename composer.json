{
  "name": "87oui/jun",
  "version": "2.0.0-alpha3",
  "type": "wordpress-theme",
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "require": {
    "inc2734/wp-breadcrumbs": "^3.1",
    "inc2734/wp-seo": "5.2.3",
    "timber/timber": "2.2.0"
  },
  "require-dev": {
    "wp-coding-standards/wpcs": "^3.0"
  },
  "scripts": {
    "format": [
      "vendor/bin/phpcbf ."
    ],
    "lint": [
      "vendor/bin/phpcs ."
    ]
  }
}
